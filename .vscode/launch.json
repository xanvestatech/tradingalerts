{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug FastAPI (Uvicorn)",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app:app",
                "--reload"
            ],
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false
        },
        {
            "name": "Debug historical_data.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/historical_data.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}
